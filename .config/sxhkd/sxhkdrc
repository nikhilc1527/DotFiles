super + t ; {a,l,d}
      dt {add,list,done}

super + shift + x
      my_lock

super + w
      $BROWSER

super + Return
      $TERM

super + d
      dmenu_run

super + BackSpace
      sysact

super + {m,p,equal}
      volume_changer output {$(( $(pamixer --get-volume) - 5 )),$(( $(pamixer --get-volume) + 5 )),50}

super + shift + {m,p,equal}
      volume_changer input {$(( $(pamixer --get-volume) - 5 )),$(( $(pamixer --get-volume) + 5 )),10}

